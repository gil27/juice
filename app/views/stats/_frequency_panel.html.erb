<div class="col-lg-4">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><i class="fa fa-clock-o fa-fw"></i> Frequency Panel</h3>
    </div>
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-bordered table-hover table-striped">
          <thead>
            <tr>
              <th># Times</th>
              <th>Term</th>
            </tr>
          </thead>
          <tbody>
          	<% Analyzer.top_terms_occurrence.each do |key, value| %>
              <tr>
                <td><%= value %></td>
                <td><%= key %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>